[alias]
  br = branch
  ca = commit --amend
  ci = commit
  cp = cherry-pick
  co = checkout
  fp = fetch --prune
  mf = merge --ff-only
  mnf = merge --no-ff
  pf = pull --ff-only
  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbi = rebase --interactive
  rh = reset --hard
  st = status
